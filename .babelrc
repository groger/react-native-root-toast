{
    "presets": ["@babel/preset-env", "@babel/preset-react", "@babel/typescript"],
    "plugins": [
        [
            "module-resolver",
            {
                "alias": {
                    "^react-native$": "react-native-web"
                }
            }
        ],
        "react-native-web",
        "transform-class-properties"
    ]
}
